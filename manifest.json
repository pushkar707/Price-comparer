{
    "manifest_version": 3,
    "name": "Price Comparer",
    "version": "1.0",
    "description": "Compares price of products you search on amazon",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.amazon.in/*"],
        "js": ["content.js"]
      }
    ]
}

